{
	"compilerOptions": {
		"allowJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"sourceMap": false,
		"baseUrl": "../.",
        "moduleResolution": "node",
        "outDir": "../dist",
        "target": "esnext",
		"paths": {
			"$server":["server"],
			"$server/*":["server/*"],
			"$lib":["src/lib"],
			"$lib/*":["src/lib/*"]
		}
	}
}