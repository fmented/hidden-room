{
	"name": "hidden-room",
	"version": "0.0.2",
	"scripts": {
		"dev": "vite dev",
		"gen": "node scripts/gen-key.js",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"build:app": "vite build",
		"build:server": "cd server && npx --yes tsc && cd .. && npx --yes copyfiles \"build/client/**/**\" dist",
		"build": "npm run build:app && npm run build:server",
		"serve": "node dist/server/server.js",

		"deploy:prepare": "node scripts/deploy-prepare.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.0.0-next.87",
		"@sveltejs/kit": "next",
		"@types/body-parser": "^1.19.2",
		"@types/express": "^4.17.13",
		"@types/uuid": "^8.3.4",
		"svelte": "^3.46.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.0.8"
	},
	"type": "module",
	"license": "SEE LICENSE IN LICENCE",
	"dependencies": {
		"express": "^4.18.1",
		"set-cookie-parser": "^2.5.1",
		"socket.io": "4.5.1",
		"socket.io-client": "4.5.1",
		"uuid": "^8.3.2"
	}
}
