{
	"name": "hidden-room",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"gen": "node scripts/gen-key.cjs",
		"build-server": "cd server && tsc server.ts --target esnext --moduleResolution node --esModuleInterop true --outDir build && cd .. && node scripts/fix-import.cjs",
		"run-server": "node server/build/server.js"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.87",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"@types/express": "^4.17.13",
		"@types/uuid": "^8.3.4",
		"express": "^4.18.1",
		"socket.io": "^4.5.1",
		"socket.io-client": "^4.5.1",
		"svelte": "^3.46.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"uuid": "^8.3.2",
		"vite": "^3.0.8"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@types/body-parser": "^1.19.2",
		"cookie": "^0.4.1"
	}
}
